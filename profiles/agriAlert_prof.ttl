@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prof: <http://www.w3.org/ns/dx/prof/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://w3id.org/demeter/agri/agriAlert> a prof:Profile ;
    dcterms:description "The DEMETER AgriAlert is one of the agri profiles of DEMETER AIM. This profile was created by reusing and aligning well-known ontologies and vocabularies"@en ;
    dcterms:source "../models/agriAlert.ttl" ;
    dcterms:title "DEMETER AgriAlert"@en ;
    rdfs:comment "The DEMETER Agriculture Information Model (AIM) is the common vocabulary in DEMETER project providing the basis for semantic interoperability across smart farming solutions"@en ;
    skos:historyNote "Ontology profile normalised using ProfileWiz" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "OWL documentation as HTML" ;
            dcterms:conformsTo prof: ;
            dcterms:format "text/html" ;
            prof:hasArtifact "agriAlert.html" ;
            prof:hasRole prof:profile ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Flattened JSON-LD context" ;
            dcterms:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dcterms:format "application/ld+json" ;
            prof:hasArtifact "agriAlert_flat.jsonld" ;
            prof:hasRole prof:contextflat ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Normalised OWL with imports" ;
            dcterms:conformsTo <http://www.w3.org/2002/07/owl#> ;
            dcterms:format "text/turtle" ;
            prof:hasArtifact "agriAlert.ttl" ;
            prof:hasRole prof:vocabulary ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "JSON-LD Context" ;
            dcterms:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dcterms:format "application/ld+json" ;
            prof:hasArtifact "agriAlert.jsonld" ;
            prof:hasRole prof:context ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "OWL with definition details from imports" ;
            dcterms:conformsTo <http://www.w3.org/2002/07/owl#> ;
            dcterms:format "text/turtle" ;
            prof:hasArtifact "agriAlert.ttl" ;
            prof:hasRole prof:vocabulary ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Profile description including links to representations" ;
            dcterms:conformsTo prof: ;
            dcterms:format "text/turtle" ;
            prof:hasArtifact "agriAlert_prof.ttl" ;
            prof:hasRole prof:profile ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Profile description as HTML" ;
            dcterms:conformsTo prof: ;
            dcterms:format "text/html" ;
            prof:hasArtifact "agriAlert_prof.html" ;
            prof:hasRole prof:profile ] .

