@prefix dct: <http://purl.org/dc/terms/> .
@prefix dim: <http://purl.oclc.org/NET/ssnx/qu/dim#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix foodie: <http://foodie-cloud.com/model/foodie#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qu: <http://purl.oclc.org/NET/ssnx/qu/qu#> .
@prefix quantity: <http://purl.oclc.org/NET/ssnx/qu/quantity#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix saref4agri: <https://w3id.org/def/saref4agri#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

foodie:MachineType rdfs:isDefinedBy <http://foodie-cloud.com/model/foodie> ;
    rdfs:subClassOf sosa:Platform .

foodie:TractorType rdfs:isDefinedBy <http://foodie-cloud.com/model/foodie> ;
    rdfs:subClassOf sosa:Platform .

quantity:criticalBuildUpSpeed a dim:RotationalSpeed,
        owl:NamedIndividual ;
    rdfs:label "critical build-up speed"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:AcousticsScheme,
        dim:PeriodicAndRelatedPhenomenaScheme .

quantity:criticalTorsionalSpeed a dim:RotationalSpeed,
        owl:NamedIndividual ;
    rdfs:label "critical torsional speed"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:AcousticsScheme,
        dim:PeriodicAndRelatedPhenomenaScheme .

quantity:criticalWhirlingSpeed a dim:RotationalSpeed,
        owl:NamedIndividual ;
    rdfs:label "critical whirling speed"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:AcousticsScheme,
        dim:PeriodicAndRelatedPhenomenaScheme .

quantity:property a qu:PropertyKind,
        owl:NamedIndividual ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

quantity:pseudovectorProperty a qu:PropertyKind,
        owl:NamedIndividual ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

quantity:rotationalSpeed a dim:RotationalSpeed,
        owl:NamedIndividual ;
    rdfs:label "rotational speed"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:AcousticsScheme,
        dim:PeriodicAndRelatedPhenomenaScheme,
        dim:SpaceAndTimeScheme .

quantity:synchronousPullOutTorque a dim:Energy,
        owl:NamedIndividual ;
    rdfs:label "synchronous pull-out torque"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:MechanicsScheme .

quantity:synchronousSpeed a dim:RotationalSpeed,
        owl:NamedIndividual ;
    rdfs:label "synchronous speed"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:AcousticsScheme,
        dim:PeriodicAndRelatedPhenomenaScheme .

quantity:torque a dim:Energy,
        owl:NamedIndividual ;
    rdfs:label "torque"^^xsd:string ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    skos:inScheme dim:MechanicsScheme .

saref4agri:EatingActivitySensor rdfs:isDefinedBy saref4agri: .

saref4agri:MilkingSensor rdfs:isDefinedBy saref4agri: .

saref4agri:MovementActivitySensor rdfs:isDefinedBy saref4agri: .

saref4agri:Pluviometer rdfs:isDefinedBy saref4agri: .

saref4agri:SoilTensiometer rdfs:isDefinedBy saref4agri: .

saref4agri:Thermometer rdfs:isDefinedBy saref4agri: .

saref4agri:WateringGun rdfs:isDefinedBy saref4agri: .

saref4agri:WateringSystem rdfs:isDefinedBy saref4agri: .

saref4agri:WateringValve rdfs:isDefinedBy saref4agri: .

saref4agri:WeatherStation rdfs:isDefinedBy saref4agri: .

saref4agri:WeightSensor rdfs:isDefinedBy saref4agri: .

<https://w3id.org/demeter/agri/agriSystem> a owl:Ontology ;
    dct:contributor [ schema:affiliation [ foaf:name "ICCS" ] ;
            foaf:name "Ioanna Roussaki" ],
        [ schema:affiliation [ foaf:name "ICCS" ] ;
            foaf:name "George Routis" ],
        [ schema:affiliation [ foaf:name "OGC" ] ;
            foaf:name "Rob Atkinson" ] ;
    dct:creator [ schema:affiliation [ foaf:name "PSNC" ] ;
            rdfs:seeAlso <http://orcid.org/0000-0003-4289-4922> ;
            foaf:name "Raul Palma" ] ;
    dct:description "The DEMETER AgriSystem is one of the agri profiles of DEMETER AIM. This profile was created by reusing and aligning well-known ontologies and vocabularies"@en ;
    dct:rights "This vocabulary is distributed under Creative Commons Attribution 4.0 License - http://creativecommons.org/licenses/by/4.0"@en ;
    dct:title "DEMETER AgriSystem"@en ;
    rdfs:comment "The DEMETER Agriculture Information Model (AIM) is the common vocabulary in DEMETER project providing the basis for semantic interoperability across smart farming solutions"@en ;
    owl:imports <http://foodie-cloud.com/model/foodie>,
        <http://www.w3.org/ns/sosa>,
        <http://www.w3.org/ns/ssn>,
        <https://w3id.org/def/saref4agri>,
        <https://w3id.org/demeter/agri/agriCommon>,
        <https://w3id.org/saref> ;
    owl:versionIRI <https://raw.githubusercontent.com/rapw3k/DEMETER/master/models/agriSystem.ttl> ;
    owl:versionInfo "1.0" ;
    foaf:maker [ foaf:homepage <https://h2020-demeter.eu/> ;
            foaf:name "DEMETER project" ] .

dim:SpaceAndTimeScheme a owl:NamedIndividual,
        skos:Scheme ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

dim:Energy a owl:Class ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    rdfs:subClassOf qu:QuantityKind .

dim:MechanicsScheme a owl:NamedIndividual,
        skos:Scheme ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

dim:AcousticsScheme a owl:NamedIndividual,
        skos:Scheme ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

dim:PeriodicAndRelatedPhenomenaScheme a owl:NamedIndividual,
        skos:Scheme ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> .

dim:RotationalSpeed a owl:Class ;
    rdfs:isDefinedBy <http://purl.oclc.org/NET/ssnx/qu/qu-rec20> ;
    rdfs:subClassOf qu:QuantityKind .

