<https://w3id.org/demeter/agri/agriFeature> a prof:Profile ;
    rdfs:label "DEMETER AgriFeature"@en ;
    dct:description "The DEMETER AgriFeature is one of the agri profiles of DEMETER AIM. This profile was created by reusing and aligning well-known ontologies and vocabularies"@en ;
    dct:title "DEMETER AgriFeature"@en ;
    rdfs:comment "The DEMETER Agriculture Information Model (AIM) is the common vocabulary in DEMETER project providing the basis for semantic interoperability across smart farming solutions"@en ;
    skos:historyNote "Ontology profile normalised using ProfileWiz" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "Normalised OWL with imports" ;
            dct:conformsTo owl: ;
            dct:format "text/turtle" ;
            rdfs:comment "This is an OWL file with imports for ontologies containing all object definitions, but with only statements not present in imports" ;
            prof:hasArtifact "agriFeature.ttl" ;
            prof:hasRole profrole:vocabulary ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Normalised OWL with imports" ;
            dct:conformsTo owl: ;
            dct:format "application/ld+json" ;
            rdfs:comment "This is an OWL file with imports for ontologies containing all object definitions, but with only statements not present in imports" ;
            prof:hasArtifact "agriFeature.jsonld" ;
            prof:hasRole profrole:vocabulary ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "JSON-LD Context" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dct:format "application/ld+json" ;
            prof:hasArtifact "agriFeature_context.jsonld" ;
            prof:hasRole profrole:context ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "JSON Schema" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_schema> ;
            dct:format "application/json" ;
            prof:hasArtifact "agriFeature.json" ;
            prof:hasRole profrole:schema ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Original Source OWL model" ;
            dct:conformsTo owl: ;
            dct:format "text/turtle" ;
            rdfs:comment "Source OWL model used to derive normalised profile views." ;
            prof:hasArtifact "../models\\agriFeature.ttl" ;
            prof:hasRole profrole:source ],
        [ a prof:ResourceDescriptor ;
            rdfs:label "Flattened JSON-LD context" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dct:format "application/ld+json" ;
            prof:hasArtifact "agriFeature_context_flat.jsonld" ;
            prof:hasRole profrole:contextflat ] ;
    prof:isProfileOf proxy:agriFeature_act-core,
        proxy:agriFeature_af,
        proxy:fiware.ttl,
        proxy:foodie.ttl,
        proxy:geosparql.ttl,
        proxy:ogc_sf.rdf,
        proxy:saref4agri.ttl,
        proxy:wgs84_pos ;
    prof:isTransitiveProfileOf <http://foodie-cloud.com/model/foodie>,
        <http://inspire.ec.europa.eu/schemas/act-core/3.0>,
        <http://inspire.ec.europa.eu/schemas/af/3.0>,
        <http://www.opengis.net/ont/geosparql>,
        <http://www.opengis.net/ont/sf>,
        <http://www.w3.org/2003/01/geo/wgs84_pos>,
        <https://uri.fiware.org/ns/data-models>,
        <https://w3id.org/def/saref4agri> ;
    prov:wasDerivedFrom "../models\\agriFeature.ttl" .

proxy:data-models a prof:Profile ;
    rdfs:label "Inferred profile of https://uri.fiware.org/ns/data-models" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "JSON-LD Context for generated profile of https://uri.fiware.org/ns/data-models" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dct:format "application/ld+json" ;
            prof:hasArtifact "extracted\\agriFeature_fiware.ttl_context.jsonld" ;
            prof:hasRole profrole:context ] ;
    prof:isProfileOf <https://uri.fiware.org/ns/data-models> .

proxy:foodie a prof:Profile ;
    rdfs:label "Foodie Ontology",
        "Inferred profile of http://foodie-cloud.com/model/foodie" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "JSON-LD Context for generated profile of http://foodie-cloud.com/model/foodie" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dct:format "application/ld+json" ;
            prof:hasArtifact "extracted\\agriFeature_foodie.ttl_context.jsonld" ;
            prof:hasRole profrole:context ],
        [ dct:conformsTo jsoncontext: ;
            dct:format "application/ld+json" ;
            rdfs:comment "self_referential as URI supports content negotation" ;
            prof:hasArtifact <https://w3id.org/demeter/agri/proxy/foodie?_profile=jsoncontext> ;
            prof:hasRole prof:schema ] ;
    prof:isProfileOf <http://foodie-cloud.com/model/foodie> .

