proxy:saref4agri a prof:Profile ;
    rdfs:label "Inferred profile of https://w3id.org/def/saref4agri",
        "DEMETER profile of SAREF extension for Agriculture"@en ;
    rdfs:comment "This profile also provides for content negotatiation behaviour to access a valid JSON-LD context" ;
    prof:hasResource [ a prof:ResourceDescriptor ;
            rdfs:label "JSON-LD Context for generated profile of https://w3id.org/def/saref4agri" ;
            dct:conformsTo <http://www.opengis.net/def/metamodel/profiles/json_ld_context> ;
            dct:format "application/ld+json" ;
            prof:hasArtifact "extracted\\agriFeature_saref4agri.ttl_context.jsonld" ;
